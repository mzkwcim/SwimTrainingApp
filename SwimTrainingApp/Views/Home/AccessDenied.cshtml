@page
@model dynamic
@{
    ViewData["Title"] = "Access Denied";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Access Denied</h1>
<p>You do not have permission to access this resource.</p>

@if (ViewBag.ReturnUrl != null && !string.IsNullOrWhiteSpace(ViewBag.ReturnUrl.ToString()))
{
    <p>
        <a asp-controller="Home" asp-action="Login">Login</a> or
        <a href="@ViewBag.ReturnUrl">Return to Previous Page</a>
    </p>
}
else
{
    <p>
        <a asp-controller="Home" asp-action="Login">Login</a>
    </p>
}